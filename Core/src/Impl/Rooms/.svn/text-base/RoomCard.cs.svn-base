namespace ManchkinQuest.Core.Impl.Rooms
{
  class RoomCard : IRoomCard
  {
    public RoomCard(INormalRoom room1, INormalRoom room2)
    {
      Side1 = room1;
      Side2 = room2;
    }

    public INormalRoom Side1 { get; private set; }
    public INormalRoom Side2 { get; private set; }
  }
}