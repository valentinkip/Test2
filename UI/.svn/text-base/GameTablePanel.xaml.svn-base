        <my:PlayerPanel x:Name="TopPlayerPanel" Height="150" HorizontalAlignment="Stretch" VerticalAlignment="Top" Margin="216,0,206,0" />
        <my:PlayerPanel x:Name="LeftPlayerPanel" Width="150" HorizontalAlignment="Left" VerticalAlignment="Stretch" Margin="0,0,0,306" />
        <my:PlayerPanel x:Name="RightPlayerPanel" Width="150" HorizontalAlignment="Right" VerticalAlignment="Stretch" Margin="0,0,0,306" />
        <my:MazePanel x:Name="MazePanel" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="156,156,156,306"  />
    	<TextBlock Name="StatusBar" HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Margin="20,710,20,0" FontSize="14" />
    </Grid>
</UserControl>
